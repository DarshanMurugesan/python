import pandas as pd

sales=pd.DataFrame({
    'Region':['North','South','East','West'],
     'Sales':[2000,3000,1500,1800]
     })

regions = pd.DataFrame({
    'Region':['North','South','East','West'],
    'Manager':['Alice','Bob','Charlie','David']
    })

regions.set_index('Region',inplace=True)

result=sales.join(regions,on='Region')
print(result)

OUTPUT:
  Region  Sales  Manager
0  North   2000    Alice
1  South   3000      Bob
2   East   1500  Charlie
3   West   1800    David
